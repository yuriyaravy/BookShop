<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.senla.bookshop.entities.Order" table="orders">
        <id column="id" name="id" type="java.lang.Long">
            <generator class="native" />
        </id>
        <property name="book" column="book_id" type="com.senla.bookshop.entities.Book" />
        <property name="dateOfDeliver" column="dateOfDeliver" type="java.lang.Date" />
        <property name="status" column="status" type="java.lang.Integer" />
 
    </class>
</hibernate-mapping>